{
    "name": "bueltge/de-de",
    "type": "wordpress-muplugin",
    "description": "WordPress-Plugin that add special german permalink sanitize and replaces characters.",
    "keywords": [
        "wordpress",
        "plugin"
    ],
    "homepage": "https://github.com/bueltge/de-DE",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Frank BÃ¼ltge",
            "email": "frank@bueltge.de",
            "homepage": "https://bueltge.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "^1.3",
        "ext-intl": "*",
        "ext-mbstring": "*"
    },
	"require-dev": {
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3",
		"wp-coding-standards/wpcs": "*",
		"phpunit/phpunit": "^8",
		"brain/monkey": "^2.6"
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"composer/installers": true,
			"roots/wordpress-core-installer": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
    "scripts": {
        "cs": "./vendor/bin/phpcs",
        "csf": "./vendor/bin/phpcbf",
        "ps": "./vendor/bin/psalm",
        "qa": [
			"@cs",
			"@ps"
		]
    }
}
